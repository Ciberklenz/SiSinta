#lista
  .botonera= render 'botonera_indice'

  = form_tag usuarios_path, method: :put do |f|

    %table#roles.centrado
      %thead
        %tr
          %th{colspan: 2} Usuario
          %th{colspan: 2} Email
          %th.acciones

      %tbody
        = render @usuarios

    = submit_tag

  .botonera= render 'botonera_indice'
