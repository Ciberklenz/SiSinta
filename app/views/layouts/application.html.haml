!!! xml
<!DOCTYPE html>
%html{'xml:lang' => 'es_AR',  'xmlns' => 'http://www.w3.org/1999/xhtml'}
  %head
    %title= "SiS INTA | #{@titulo}"
    = stylesheet_link_tag     "application", :media => 'screen, print'
    = stylesheet_link_tag     "print", :media => 'print'
    = stylesheet_link_tag     "ie" if @ie
    = javascript_include_tag  "application"
    = csrf_meta_tags
  %body.celeste
    #cabecera= render 'layouts/cabecera'
    .container
      #principal.span-24
        = yield
